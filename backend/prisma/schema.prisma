generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model users {
  id       Int    @id @default(autoincrement()) @db.UnsignedInt
  username String @db.VarChar(255)
  password String @db.VarChar(200)
  email    String @db.VarChar(255)
}
